<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.midea.cloud.srm.base.purchase.mapper.PurchaseCurrencyMapper">

    <select id="listAllCurrencyForImport" resultType="com.midea.cloud.srm.model.base.purchase.entity.PurchaseCurrency">
